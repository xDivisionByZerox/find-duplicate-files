  "homepage": "https://github.com/xDivisionByZerox/find-duplicate-images#readme",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^8.13.0",
    "ts-loader": "^9.2.6",
